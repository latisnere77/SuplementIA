{
    "name": "weaviate-admin-lambda",
    "version": "1.0.0",
    "main": "index.js",
    "dependencies": {
        "@aws-sdk/client-ecs": "^3.0.0",
        "@aws-sdk/client-ec2": "^3.0.0",
        "jose": "^5.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0",
        "esbuild": "^0.19.0"
    },
    "scripts": {
        "build": "esbuild weaviate-admin.ts --bundle --platform=node --target=node20 --outfile=dist/admin/index.js --external:@aws-sdk/* && esbuild weaviate-authorizer.ts --bundle --platform=node --target=node20 --outfile=dist/authorizer/index.js",
        "package": "cd dist/admin && zip -r ../../weaviate-admin.zip . && cd ../authorizer && zip -r ../../weaviate-authorizer.zip ."
    }
}