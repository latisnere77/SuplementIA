{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_observations",
        "delete_entities",
        "delete_relations",
        "read_graph",
        "search_nodes",
        "open_nodes",
        "add-note"
      ]
    },
    "sentry": {
      "command": "npx",
      "args": [
        "-y",
        "@sentry/mcp-server@latest",
        "--access-token=${SENTRY_ACCESS_TOKEN}"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "mcp_sentry_whoami",
        "mcp_sentry_find_organizations",
        "mcp_sentry_find_teams",
        "mcp_sentry_find_projects",
        "mcp_sentry_find_releases",
        "mcp_sentry_get_issue_details",
        "mcp_sentry_get_trace_details",
        "mcp_sentry_search_events",
        "mcp_sentry_search_issues",
        "mcp_sentry_search_docs",
        "mcp_sentry_get_doc"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}