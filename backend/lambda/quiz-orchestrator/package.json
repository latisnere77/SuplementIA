{
  "name": "quiz-orchestrator",
  "version": "1.0.0",
  "description": "Quiz orchestrator Lambda - coordinates search, enrichment, and recommendations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "zip": "cd dist && zip -r ../quiz-orchestrator.zip .",
