{
  "name": "suplementia-studies-fetcher",
  "version": "1.0.0",
  "description": "Fetch real scientific studies from PubMed for supplements",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "package": "npm run build && npm run package:zip",
    "package:zip": "cd dist && zip -r ../studies-fetcher.zip . && cd ..",
    "deploy": "npm run package && aws lambda update-function-code --function-name suplementia-studies-fetcher --zip-file fileb://studies-fetcher.zip"
  },
  "keywords": ["pubmed", "studies", "research", "scientific"],
  "author": "Suplementia",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.490.0",
    "aws-xray-sdk-core": "^3.5.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
